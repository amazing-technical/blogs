{
  "root": "./books",
  "title": "Amazing Techical",
  "author": "AmazingTechnical",
  "plugins": [
    "git-author",
    "chapter-fold",
    "page-treeview",
    "accordion"
  ],
  "pluginsConfig": {
    "git-author": {
      "position": "top",
      "modifyTpl": "Last modified by {user} {timeStamp}",
      "createTpl": false,
      "timeStampFormat": "YYYY-MM-DD"
    },
    "page-treeview": {
      "minHeaderCount": "1",
      "minHeaderDeep": "1"
    }
  },
  "styles": {
    "website": "./styles/website.css"
  }
}